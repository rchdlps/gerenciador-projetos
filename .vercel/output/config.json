{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/api(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/calendar/?$",
			"dest": "_render"
		},
		{
			"src": "^/knowledge-areas/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/projects/([^/]+?)/calendar/?$",
			"dest": "_render"
		},
		{
			"src": "^/projects/([^/]+?)/knowledge-areas/?$",
			"dest": "_render"
		},
		{
			"src": "^/projects/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/register/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}